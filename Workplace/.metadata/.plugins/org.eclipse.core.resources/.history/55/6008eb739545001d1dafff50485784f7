//					System.out.println(" The Value of array ["+i+"]"+"= "+array1[i]);
//				}
				int[]array2= {10,20,30,40,50};
				for(int i=0;i<array2.length;i++) {
					System.out.print(array2[i]+" ");
				}System.out.println();
				int[] array7=array2.clone();
				for(int i=0;i<array7.length;i++) {
					System.out.print(array7[i]);
				}
				Arrays.sort(array2);
				System.out.println();
				for(int a:array2) {
					System.out.print(a+" ");
				}
				System.out.println();
				int[] array3=Arrays.copyOf(array2,2);
				for(int a1:array3) {
					System.out.print(a1+" ");
				}
				System.out.println();
				int[]array4=Arrays.copyOfRange(array2, 1, 3);
				for(int a2:array4) {
					System.out.print(a2+" ");
				}
				int[] array5={20,30};
				System.out.println();
//				System.out.println(array3.equals(array4));
//				System.out.println(array3==array4);
				//System.out.println(array4.equals(array5));
				System.out.println(Arrays.equals(array4, array5));
				int[] array6=array5.clone();
				for(int i=array6.length-1;i>=0;i--) {
					System.out.print(array6[i]+" ");
				}
				
				
				//System.out.println(array4==array5);
				//System.out.println(array4==array5);
//		array1[0]=10;
//		array1[1]=12;
//		array1[2]=16;
